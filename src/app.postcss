/* Write your global styles here, in PostCSS syntax */
@tailwind base;
@tailwind components;
@tailwind utilities;

body {
  padding: 0;
  margin: 0;
  position: absolute;
  height: 100%;
  width: 100%;
}

* {
  font-family: Arial, Helvetica, sans-serif;
}

html,
body {
  @apply h-full overflow-hidden;
}
